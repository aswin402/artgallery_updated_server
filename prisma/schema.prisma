generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model Art{
    id    Int     @id @default(autoincrement())
    artname String
    artist String
    price Int
    imageUrl    String?
}

